{

    "name": "YouTubeAd Skipper",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "広告自動スキップ",
    "content_scripts": [{
        "matches": [ "http://www.youtube.com/*", "https://www.youtube.com/*" ],
        "js": ["youtube_auto_skip.js"]
    }],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": ["activeTab"]
}
